{
  "name": "webpack-builder-boilerplate",
  "version": "1.2.2",
  "description": "Boilerplate for webpack build of browser application",
  "repository": "git@github.com:360fy/webpack-builder-boilerplate.git",
  "scripts": {
    "lint": "eslint ./src/",
    "clean": "rm -rf lib/",
    "compile": "npm run lint && npm run clean && babel -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "author": "Shailendra Sharma",
  "license": "MIT",
  "dependencies": {
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.0",
    "babel-loader": "6.2.7",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-node5": "11.1.0",
    "babel-preset-react": "6.16.0",
    "chunk-manifest-webpack-plugin": "0.1.0",
    "css-loader": "0.26.0",
    "eslint": "3.10.2",
    "eslint-config-360fy": "1.1.12",
    "eslint-loader": "1.6.1",
    "eslint-plugin-react": "6.7.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "json-loader": "0.5.4",
    "lodash": "4.17.2",
    "node-sass": "3.13.0",
    "react": "15.4.0",
    "react-dom": "15.4.0",
    "react-hot-loader": "3.0.0-beta.6",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "webpack": "1.13.3",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.2",
    "webpack-manifest-plugin": "1.1.0",
    "webpack-md5-hash": "0.0.5"
  },
  "eslintConfig": {
    "extends": "360fy"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "node5",
      "react"
    ]
  },
  "devDependencies": {}
}
