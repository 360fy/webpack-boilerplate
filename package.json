{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for webpack build of browser application",
  "repository": "git@github.com:360fy/webpack-boilerplate.git",
  "scripts": {
    "lint": "eslint ./src/",
    "clean": "rm -rf lib/",
    "compile": "npm run lint && npm run clean && babel -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "author": "Shailendra Sharma",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-node5": "^11.0.0",
    "babel-preset-react": "^6.5.0",
    "chunk-manifest-webpack-plugin": "0.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.4.0",
    "eslint-config-360fy": "^1.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "react": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-manifest-plugin": "^1.0.0",
    "webpack-md5-hash": "0.0.5"
  },
  "eslintConfig": {
    "extends": "360fy"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "node5",
      "react"
    ]
  }
}
