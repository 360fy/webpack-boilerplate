{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for webpack build of browser application",
  "private": true,
  "repository": "git+ssh://git-codecommit.us-east-1.amazonaws.com/v1/repos/webpack-boilerplate",
  "scripts": {
    "lint": "eslint ./src/",
    "clean": "rm -rf lib/",
    "compile": "npm run lint && npm run clean && babel -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "author": "Shailendra Sharma",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-node5": "^10.5.0",
    "babel-preset-react": "^6.3.13",
    "chunk-manifest-webpack-plugin": "0.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-360fy": "https://private-npm-releases.s3.amazonaws.com/eslint-config-360fy-1.0.0.tgz",
    "eslint-loader": "^1.2.1",
    "eslint-plugin-react": "^3.16.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "react": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.12",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.6.4",
    "webpack-manifest-plugin": "^0.5.0",
    "webpack-md5-hash": "0.0.4"
  },
  "eslintConfig": {
    "extends": "360fy"
  },
  "babel": {
    "presets": [
      "node5",
      "react"
    ]
  }
}
